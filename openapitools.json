{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "6.4.0",
    "generators": {
      "v3.0.1": {
        "generatorName": "typescript-node",
        "output": "src",
        "glob": "openapi.json",
        "additionalProperties": {
          "modelPropertyNaming": "camelCase",
          "supportsES6": true
        }
      }
    }
  }
}